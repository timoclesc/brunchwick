{
  "id": "article",
  "label": "Article",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": "URL slug for the article"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "Title of the article",
          "allowTargetBlank": false,
          "single": "heading1"
        }
      },
      "publishDate": {
        "type": "Date",
        "config": {
          "label": "Publish Date",
          "placeholder": "Date the article was published"
        }
      },
      "featuredImage": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {
            "width": 2000,
            "height": 1500
          },
          "thumbnails": []
        }
      },
      "excerpt": {
        "type": "Text",
        "config": {
          "label": "Excerpt",
          "placeholder": ""
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "image": {
              "type": "SharedSlice"
            },
            "quote": {
              "type": "SharedSlice"
            },
            "text": {
              "type": "SharedSlice"
            },
            "menu_item": {
              "type": "SharedSlice"
            },
            "weekly_specials": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "Contact Info": {
      "google_maps": {
        "type": "Link",
        "config": {
          "label": "Google Maps",
          "placeholder": "",
          "select": null
        }
      },
      "website": {
        "type": "Link",
        "config": {
          "label": "Website",
          "placeholder": "",
          "select": null
        }
      }
    },
    "Social": {
      "instagram_image": {
        "type": "Image",
        "config": {
          "label": "Instagram Image",
          "constraint": {
            "width": 936,
            "height": 936
          },
          "thumbnails": []
        }
      },
      "instagram_text": {
        "type": "Text",
        "config": {
          "label": "Instagram Text",
          "placeholder": ""
        }
      }
    }
  }
}