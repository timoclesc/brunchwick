{
  "name": "brunchwick.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:next:dev\" \"npm:slicemachine\" --names \"next,slicemachine\" --prefix-colors blue,magenta",
    "next:dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint './src/**/*.js'",
    "slicemachine": "start-slicemachine",
    "format": "prettier --write .",
    "gen": "prismic-ts-codegen",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@prismicio/client": "^6.7.3",
    "@prismicio/helpers": "^2.3.8",
    "@prismicio/next": "^1.0.2",
    "@prismicio/react": "^2.5.1",
    "@prismicio/slice-simulator-react": "^0.2.3",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "concurrently": "^7.6.0",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "next": "13.1.1",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.44",
    "prettier": "^2.8.1",
    "prismic-ts-codegen": "^0.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.6",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@prismicio/types": "^0.2.7",
    "babel-plugin-styled-components": "^2.0.7",
    "husky": "^8.0.3",
    "slice-machine-ui": "^0.6.0",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
